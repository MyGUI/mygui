material MyGUI_pointerAll
{
	technique
	{
		pass
		{
			lighting off
			ambient 1 1 1
			diffuse 1 1 1
			specular 1 1 1 30
			scene_blend alpha_blend
			texture_unit
			{
				filtering linear linear none
				texture SkinPointer\MyGUI_PointerAll.png
			}
		}
	}
}



